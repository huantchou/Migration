package com.esb.saml.cmt.enums;

public enum AlertReviewStatusEnum {

    /**
     * 待審查
     */
    PENDING("Pending"),
    /**
     * 待覆核
     */
    REVIEW("Review"),
    
    /**
     * 結案
     */
    CLOSE("Close");
    
    /**
     * 列舉文字
     */
    private String context;

    /**
     * 名單篩選方式列舉文字
     * @param context 列舉文字
     */
    private AlertReviewStatusEnum(String context) {
        this.context = context;
    }

    /**
     * 取得列舉對應文字
     * @return context 列舉文字
     */
    public String getContext() {
        return context;
    }

}
