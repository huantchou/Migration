package com.esb.saml.migration.controller.alertManagement;

import java.util.List;

import org.slf4j.Logger;
import org.slf4j.LoggerFactory;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RestController;

import com.esb.saml.migration.service.MigrationService;

@RestController
@RequestMapping(value = "/migration")
public class MgrAlertManagementController {

	@Autowired
	private MigrationService migrationService;

	@RequestMapping(value = "/samlCloseAlert")
	public List<Long> samlCloseAlert() throws Exception {
		return migrationService.samlCloseAlert();
	}

}
